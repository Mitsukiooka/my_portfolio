.header
  %h1
    Carrier
.contents
  %h2="Self introduction"
  %p
.contents
  %h2="Education"
  -data.educations.each do |education|
    %h3=education[:name]
    %p=education[:period]
    %span=education[:degree]
    %p=education[:major]
.contents
  %h2="Work Experiences"
  -data.exp_companies.each do |exp_company|
    %h3=exp_company[:name]
    %p=exp_company[:period]
    %span=exp_company[:role]
    %p=exp_company[:job]
.contents
  %h2="Ativities"
  -data.activities.each do |activity|
    %h3=activity[:name]
    %p=activity[:period]
    %p=activity[:content]